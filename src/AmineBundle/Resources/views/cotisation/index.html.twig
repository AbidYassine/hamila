{% extends 'base.html.twig' %}

{% block body %}

    <div class="container">
        <h1>Cotisations list</h1>

    <table class="display nowrap table table-hover table-striped table-bordered">
        <thead>
            <tr>
                <th>Prixht</th>
                <th>Prixttc</th>
                <th>Tva</th>
                <th>Datedebutcotisation</th>
                <th>Datefincotisation</th>
                <th>Paiement</th>
            </tr>
        </thead>
        <tbody>
        {% for cotisation in cotisations %}
            <tr>
                <td>{{ cotisation.prixHt }}</td>
                <td>{{ cotisation.prixTtc }}</td>
                <td>{{ cotisation.tva }}</td>
                <td>{{ cotisation.dateDebutCotisation }}</td>
                <td>{{ cotisation.dateFinCotisation }}</td>
                <td>{{ cotisation.paiement }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
{% endblock %}
